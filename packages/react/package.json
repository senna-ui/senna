{
  "name": "@senna-ui/react",
  "sideEffects": false,
  "version": "0.0.6",
  "private": false,
  "description": "React specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/senna-ui/senna.git",
    "directory": "packages/react"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "jest": "^26.4.2",
    "jest-dom": "^3.0.2",
    "np": "^6.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@senna-ui/core": "^0.0.6"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
