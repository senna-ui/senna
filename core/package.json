{
  "name": "@senna-ui/core",
  "version": "0.0.6",
  "description": "Senna UI core framework",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/senna/senna.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "docs:json": "stencil build --docs-json",
    "docs": "npm run build && npm run docs:json",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s ./www",
    "build-storybook": "build-storybook",
    "lint": "eslint src/**/*{.ts,.tsx}"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@stencil/core": "^2.0.3",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.3.2",
    "@storybook/addon-actions": "^6.0.22",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/html": "^6.0.22",
    "@storybook/preset-typescript": "^3.0.0",
    "@types/jest": "^26.0.14",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.2",
    "puppeteer": "^5.3.1",
    "typescript": "^4.0.3"
  },
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/senna-ui/senna.git",
    "directory": "core"
  }
}
